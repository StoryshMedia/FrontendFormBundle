<div {% if additionalClasses is defined %}class="{{ additionalClasses }}"{% endif %}>
    {% if headline.value is not empty %}
        <h3 class="{{ headline.classes }}" {% if id is defined %} {{ adminConfig(_context, 'headline', id) }} {% endif %}>
            {{ headline.value }}
        </h3>
    {% endif %}
    <div
        id="dynamic-form-{% if id is defined %}{{ id }}{% endif %}" {% if id is defined %} {{ adminConfig(_context, '', id) }} {% endif %}
        data-props="{{ pluginSettingsJson }}"
        {% if id is defined %} data-frontendId="{{ id }}" {% endif %} {% if id is defined %}class="administration--plugin-overlay"{% endif %}>
    </div>
    {{ smug_encore_entry_script_tags('dynamicForm', null, '_default', [], true) }}
</div>